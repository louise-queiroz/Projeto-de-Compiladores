void main() {
    newVar float limit;
    newVar float num;
    newVar float i;
    newVar float isPrime;

    limit = 20.0; 
    num = 2.0;

    while (num <= limit) {
        isPrime = 1.0;
        i = 2.0;

        while (i <= num / 2.0) {
            if (num / i == 0.0) {
                isPrime = 0.0;
                break;
            }
            i = i + 1.0;
        }

        if (isPrime == 1.0) {
            printOut num;
        }

        num = num + 1.0;
    }
}
