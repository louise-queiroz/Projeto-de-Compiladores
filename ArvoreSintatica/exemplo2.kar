void main() {
    newVar float limit;
    newVar float a;
    newVar float b;
    newVar float next;
    newVar float i;

    limit = 20.0;  
    a = 0.0; 
    b = 1.0;  

    printOut a; 
    printOut b;  

    i = 2.0; 
    while (i < limit) {  
        next = a + b;  
        printOut next;
        a = b; 
        b = next;  
        i = i + 1.0;  
    }
}
