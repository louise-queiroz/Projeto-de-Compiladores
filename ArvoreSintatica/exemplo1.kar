
    newVar float num;
    newVar float fatorial;
    newVar float i;

    num = 5.0;  // Número para o qual calcular o fatorial
    fatorial = 1.0;  // Inicializa o fatorial como 1

    i = 1.0;  // Começa a contagem em 1
    while (i < num + 1.0) {  // Continua enquanto i for menor que num + 1
        fatorial = fatorial * i;  // Multiplica o fatorial pelo número atual
        i = i + 1.0;  // Incrementa i
    }

    printOut fatorial;  // Imprime o resultado do fatorial

